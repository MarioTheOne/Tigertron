/*Este programa imprime 5 bloques de 5 líneas cada uno,
	donde en cada línea están los # del 1 al 10 multiplicados
	por # de línea y # de bloque*/

(
for j:= 1 to 10 do
(
	for k:= 1 to 10 do
	(
		for l:= 1 to 10 do
			( printi(j*k*l); print(" ") );
		print("\n");
		if k = 5 then break
	);
	
	print("----------------------\n");
	if j = 5 then break);
getchar()
)
